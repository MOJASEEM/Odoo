<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_man_qual_tree" model="ir.ui.view">
        <field name="name">man.qual.tree</field>
        <field name="model">man.qual</field>
        <field name="arch" type="xml">
            <tree>
                <field name="qual"/>
            </tree>
        </field>
    </record>

    <record id="view_man_qual_form" model="ir.ui.view">
        <field name="name">man.qual.form</field>
        <field name="model">man.qual</field>
        <field name="arch" type="xml">
            <form string="Qual Master">
                <sheet>
                    <group>
                        <field name="qual"/>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="action_qual_details" model="ir.actions.act_window">
        <field name="name">qual Model</field>
        <field name="res_model">man.qual</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>